{# templates/contact/form.html.twig #}

{% extends 'base.html.twig' %}

{% block body %}
    <h2>Contact Form</h2>
    {{ form_start(form) }}
    {{ form_row(form.name) }}
    {{ form_row(form.email) }}
    {{ form_row(form.question) }}
    <button type="submit">Submit</button>
    {{ form_end(form) }}
{% endblock %}